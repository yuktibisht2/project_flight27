{
  "_version": "1.65.0",
  "sap.app": {
    "id": "app.capgepost27",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.17.3",
      "toolsId": "281b23f3-8899-44a4-9d38-25b459d9a4ae"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZPROJECT_700_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "flightdata-manage": {
          "semanticObject": "flightdata",
          "action": "manage",
          "title": "{{flightdata-manage.flpTitle}}",
          "subTitle": "{{flightdata-manage.flpSubtitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.134.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "app.capgepost27.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      },
      "CustomerModel":{
"type":"sap.ui.model.json.JSONModel"}

      
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "app.capgepost27.view",
        "async": true,
        "viewPath": "app.capgepost27.view"
      },
      "routes": [
        {
          "name": "RouteCustomerViewb27",
          "pattern": ":?query:",
          "target": [
            "TargetCustomerViewb27"
          ]
        },
        {
          "name": "RouteCreateView",
          "pattern": "create",
          "target": [
            "TargetCreateView"
          ]
        },
        {
        "name": "RouteDetailView",
          "pattern": "detail/{indexDetail}",
          "target": [
            "TargetDetailView"   
          ]
        },
        {
          "name": "RouteUpdateView",
          "pattern": "update/{indexUpdate}",
          "target": [
            "TargetUpdateView"
          ]
        }
      ],
      "targets": {
        "TargetCustomerViewb27": {
          "id": "CustomerViewb27",
          "name": "CustomerViewb27"
        },
          "TargetCreateView": {
            "viewId": "idCreatePage",
            "viewName": "CreateView"
        },
        "TargetDetailView":{
          "viewId":"idDetailPage",
          "viewName":"DetailView"
        },
        "TargetUpdateView": {
          "viewId": "idUpdate",
          "viewName": "UpdateView"
        }



      }
      },
    "rootView": {
      "viewName": "app.capgepost27.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "appcapgepost27"
  }
}
